<?xml version='1.0'?>

<trace enabled="yes"
 initial-mode="detail"
 type="paraver"
 xml-parser-id="Id: xml-parse.c 3918 2016-03-11 14:59:01Z harald $"
>

  <user-functions enabled="no" list="" exclude-automatic-functions="no">
  </user-functions>

  <pthread enabled="no">
    <counters enabled="yes" />
  </pthread>

  <counters enabled="yes">
    <cpu enabled="yes" starting-set-distribution="1">
      <set enabled="yes" domain="all" changeat-time="0ms">
        PAPI_TOT_INS,PAPI_TOT_CYC,PAPI_L1_DCM,PAPI_L2_DCM,PAPI_L3_TCM,PAPI_BR_INS,PAPI_BR_MSP,RESOURCE_STALLS
      </set>
    </cpu>
  </counters>

  <!-- comented cpu events for java threads (remove comments to activate
  <cpu-events enabled="yes" frequency="10u" emit-always="no"/>
  -->
  
  <storage enabled="yes">
    <trace-prefix enabled="no">TRACE</trace-prefix>
    <size enabled="no">5</size>
    <temporal-directory enabled="yes">{{TRACE_OUTPUT_DIR}}</temporal-directory>
    <final-directory enabled="yes">{{TRACE_OUTPUT_DIR}}</final-directory>
  </storage>


  <buffer enabled="yes">
    <size enabled="yes">100000</size>
    <circular enabled="no"/>
  </buffer>

  <trace-control enabled="no"/>

  <others enabled="no"/>

  <sampling enabled="no"/>

  <dynamic-memory enabled="no"/>

  <input-output enabled="no"/>

  <merge enabled="no"/>

</trace>
